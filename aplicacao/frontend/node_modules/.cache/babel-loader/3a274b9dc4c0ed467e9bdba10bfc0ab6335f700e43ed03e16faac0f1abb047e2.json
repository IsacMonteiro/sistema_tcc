sContent":["<template>\r\n  <v-container>\r\n    <!-- Título do Formulário -->\r\n    <v-row justify=\"center\" class=\"mb-4\">\r\n      <v-col cols=\"12\" md=\"8\">\r\n        <h2 class=\"form-title text-center\">Cadastro de Obra</h2>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Formulário -->\r\n    <v-form ref=\"obraForm\" v-model=\"valid\">\r\n      <v-row justify=\"center\" class=\"form-container\" no-gutters>\r\n        <!-- Coluna 1 -->\r\n        <v-col cols=\"12\" md=\"5\" class=\"pr-md-2\">\r\n          <!-- Título -->\r\n          <v-text-field\r\n            label=\"Título da Obra\"\r\n            v-model=\"obra.titulo\"\r\n            :rules=\"[rules.required]\"\r\n            outlined\r\n            dense\r\n          ></v-text-field>\r\n\r\n          <!-- Tipo -->\r\n          <v-select\r\n            label=\"Tipo de Obra\"\r\n            v-model=\"obra.tipo\"\r\n            :items=\"tipos\"\r\n            item-text=\"nome\"\r\n            item-value=\"id\"\r\n            :rules=\"[rules.required]\"\r\n            outlined\r\n            dense\r\n          ></v-select>\r\n\r\n          <!-- Data de Apresentação -->\r\n          <v-menu\r\n            v-model=\"menu\"\r\n            :close-on-content-click=\"false\"\r\n            :nudge-width=\"200\"\r\n            offset-y\r\n          >\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-text-field\r\n                label=\"Data de Apresentação\"\r\n                v-model=\"obra.data_apresentacao\"\r\n                :rules=\"[rules.required]\"\r\n                readonly\r\n                placeholder=\"Escolha uma data\"\r\n                v-bind=\"attrs\"\r\n                v-on=\"on\"\r\n                outlined\r\n                dense\r\n              ></v-text-field>\r\n            </template>\r\n            <v-date-picker\r\n              v-model=\"obra.data_apresentacao\"\r\n              @input=\"menu = false\"\r\n              scrollable\r\n            ></v-date-picker>\r\n          </v-menu>\r\n\r\n          <!-- Resumo -->\r\n          <v-textarea\r\n            label=\"Resumo\"\r\n            v-model=\"obra.resumo\"\r\n            :rules=\"[rules.required]\"\r\n            outlined\r\n            dense\r\n            rows=\"3\"\r\n          ></v-textarea>\r\n\r\n          <!-- Palavras-Chave -->\r\n          <v-text-field\r\n            label=\"Palavras-Chave\"\r\n            v-model=\"obra.palavras_chave\"\r\n            :rules=\"[rules.keywords]\"\r\n            outlined\r\n            dense\r\n            hint=\"Separe por vírgulas (Ex.: TI, Repositório, TCC)\"\r\n          ></v-text-field>\r\n\r\n          <!-- Arquivo -->\r\n          <v-file-input\r\n            label=\"Arquivo\"\r\n            v-model=\"obra.arquivo\"\r\n            outlined\r\n            dense\r\n            accept=\".pdf\"\r\n          ></v-file-input>\r\n        </v-col>\r\n\r\n        <!-- Coluna 2 -->\r\n        <v-col cols=\"12\" md=\"5\" class=\"pl-md-2\">\r\n          <!-- Curso -->\r\n          <v-select\r\n            label=\"Curso\"\r\n            v-model=\"obra.fk_id_curso\"\r\n            :items=\"cursos\"\r\n            item-text=\"nome\"\r\n            item-value=\"id\"\r\n            :rules=\"[rules.required]\"\r\n            outlined\r\n            dense\r\n          ></v-select>\r\n\r\n          <!-- Autor -->\r\n          <v-text-field\r\n            label=\"Autor\"\r\n            v-model=\"obra.autor\"\r\n            :rules=\"[rules.required]\"\r\n            outlined\r\n            dense\r\n          ></v-text-field>\r\n\r\n          <!-- Orientador -->\r\n          <v-text-field\r\n            label=\"Orientador\"\r\n            v-model=\"obra.orientador\"\r\n            :rules=\"[rules.required]\"\r\n            outlined\r\n            dense\r\n          ></v-text-field>\r\n\r\n          <!-- Possui Coorientador -->\r\n          <v-switch\r\n            label=\"Possui Coorientador?\"\r\n            v-model=\"obra.possuiCoorientador\"\r\n            inset\r\n            dense\r\n          ></v-switch>\r\n\r\n          <!-- Coorientador Exibido Condicionalmente -->\r\n          <v-text-field\r\n            v-if=\"obra.possuiCoorientador\"\r\n            label=\"Coorientador\"\r\n            v-model=\"obra.coorientador\"\r\n            outlined\r\n            dense\r\n          ></v-text-field>\r\n        </v-col>\r\n      </v-row>\r\n\r\n      <!-- Botões -->\r\n      <v-row justify=\"center\" class=\"mt-4\">\r\n        <v-col cols=\"12\" md=\"8\" class=\"text-center\">\r\n          <v-btn class=\"mr-4\" id=\"save\" @click=\"handleSubmit\">Salvar</v-btn>\r\n          <v-btn outlined class=\"mr-4\" id=\"reset\" @click=\"handleReset\">Limpar</v-btn>\r\n          <v-btn outlined id=\"back\" @click=\"goBack\">Voltar</v-btn>\r\n        </v-col>\r\n      </v-row>\r\n    </v-form>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      valid: false,\r\n      menu: false, // Controle do estado do calendário\r\n      obra: {\r\n        titulo: \"\",\r\n        tipo: \"\",\r\n        data_apresentacao: null, // Inicializado como nulo\r\n        resumo: \"\",\r\n        palavras_chave: \"\",\r\n        arquivo: null,\r\n        fk_id_curso: null,\r\n        autor: \"\",\r\n        orientador: \"\",\r\n        coorientador: \"\",\r\n        possuiCoorientador: false, // Controle do switch de coorientador\r\n      },\r\n      cursos: [], // Lista de cursos para popular o select\r\n      tipos: [], // Lista de tipos de obra\r\n      rules: {\r\n        required: (value) => !!value || \"Este campo é obrigatório\",\r\n        keywords: (value) =>\r\n          (value && value.split(\",\").length > 0) ||\r\n          \"Informe ao menos uma palavra-chave\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    handleSubmit() {\r\n      if (this.$refs.obraForm.validate()) {\r\n        console.log(\"Dados do formulário:\", this.obra);\r\n        // Lógica para enviar dados ao backend\r\n      }\r\n    },\r\n    handleReset() {\r\n      this.$refs.obraForm.reset();\r\n      Object.keys(this.obra).forEach((key) => (this.obra[key] = null));\r\n      this.obra.possuiCoorientador = false; // Resetar o switch\r\n    },\r\n    goBack() {\r\n      // Redireciona para a página HomePageAdm\r\n      this.$router.push(\"/HomePageAdm\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-title {\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  text-transform: uppercase;\r\n  border-radius: 8px;\r\n  padding: 10px;\r\n  margin-top: 30px;\r\n  margin-bottom: 20px;\r\n  color: #fff;\r\n  background: linear-gradient(90deg, #00420c, #28a745);\r\n}\r\n\r\n/* Botões */\r\n#save {\r\n  background-color: #006dba;\r\n  color: #fff;\r\n  font-weight: bold;\r\n}\r\n\r\n#reset {\r\n  background-color: red;\r\n  color: #fff;\r\n  font-weight: bold;\r\n}\r\n\r\n#back {\r\n  background-color: #00420c;\r\n  color: #fff;\r\n}\r\n\r\n#save: {\r\n  background-color: #006dba;\r\n  color: #fff;\r\n  font-weight: bold;\r\n}\r\n\r\n#reset {\r\n  background-color: red;\r\n  color: #fff;\r\n  font-weight: bold;\r\n}\r\n\r\n#back {\r\n  background-color: #00420c;\r\n  color: #fff;\r\n}\r\n\r\n/* Formulário */\r\n.form-container {\r\n  max-width: 1170px;\r\n  border-radius: 10px;\r\n  margin: auto;\r\n  padding: 10px;\r\n  background-color: #f9f9f9;\r\n  color: #002907;\r\n}\r\n</style>"],"mappings":";AA4JA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,KAAK;MACZC,IAAI,EAAE,KAAK;MAAE;MACbC,IAAI,EAAE;QACJC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,iBAAiB,EAAE,IAAI;QAAE;QACzBC,MAAM,EAAE,EAAE;QACVC,cAAc,EAAE,EAAE;QAClBC,OAAO,EAAE,IAAI;QACbC,WAAW,EAAE,IAAI;QACjBC,KAAK,EAAE,EAAE;QACTC,UAAU,EAAE,EAAE;QACdC,YAAY,EAAE,EAAE;QAChBC,kBAAkB,EAAE,KAAK,CAAE;MAC7B,CAAC;MACDC,MAAM,EAAE,EAAE;MAAE;MACZC,KAAK,EAAE,EAAE;MAAE;MACXC,KAAK,EAAE;QACLC,QAAQ,EAAGC,KAAK,IAAK,CAAC,CAACA,KAAI,IAAK,0BAA0B;QAC1DC,QAAQ,EAAGD,KAAK,IACbA,KAAI,IAAKA,KAAK,CA