v-divider>\r\n        <!-- Título do Administrador -->\r\n        <v-title depressed class=\"title-adm\">Sou administrador:</v-title>\r\n        <!-- Botão Entrar -->\r\n        <v-btn @click=\"toggleLoginForm\" class=\"login-btn ml-3\">ENTRAR</v-btn>\r\n      </div>\r\n    </v-container>\r\n  </v-app-bar>\r\n\r\n  <!-- Formulário de Login -->\r\n  <v-dialog v-model=\"loginFormVisible\" max-width=\"500px\">\r\n    <v-card>\r\n      <v-card-title class=\"text-center\">\r\n        <span class=\"headline\">ÁREA ADMINIS</span>\r\n        <!-- Ícone de fechar -->\r\n        <v-btn\r\n          icon\r\n          class=\"close-btn\"\r\n          @click=\"loginFormVisible = false\"\r\n        >\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n      </v-card-title>\r\n      <v-card-text>\r\n        <v-form @submit.prevent=\"submitLogin\">\r\n          <v-text-field\r\n            label=\"E-mail\"\r\n            v-model=\"loginData.email\"\r\n            type=\"email\"\r\n            required\r\n          ></v-text-field>\r\n          <v-text-field\r\n            label=\"Senha\"\r\n            v-model=\"loginData.password\"\r\n            type=\"password\"\r\n            required\r\n          ></v-text-field>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-card-actions class=\"d-flex justify-center\">\r\n        <!-- Botão Confirmar -->\r\n        <v-btn color=\"primary\" @click=\"submitLogin\" class=\"confirm-btn\">\r\n          Entrar\r\n        </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      loginFormVisible: false, // Controla a visibilidade do formulário de login\r\n      loginData: {\r\n        email: \"\",\r\n        password: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    toggleLoginForm() {\r\n      // Alterna a visibilidade do formulário de login\r\n      this.loginFormVisible = !this.loginFormVisible;\r\n    },\r\n    submitLogin() {\r\n      // Aqui você pode processar o login\r\n      console.log(\"Email:\", this.loginData.email);\r\n      console.log(\"Senha:\", this.loginData.password);\r\n      // Você pode adicionar lógica de autenticação aqui\r\n\r\n      // Após o login, fecha o formulário\r\n      this.loginFormVisible = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.v-app-bar {\r\n  background: #005f00;\r\n  padding: 10px 0;\r\n}\r\n\r\n.title {\r\n  background-color: #00420c;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  font-weight: bold;\r\n  color: #fff;\r\n}\r\n\r\n.title-link {\r\n  text-decoration: none; /* Remove o sublinhado */\r\n}\r\n\r\n.logo-img {\r\n  width: 50px;\r\n  height: 50px;\r\n}\r\n\r\n.title-adm {\r\n  font-weight: bold;\r\n  color: #fff;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.search-input {\r\n  max-width: 300px;\r\n  width: 300px;\r\n  border-radius: 10px;\r\n  padding: 5px 10px;\r\n  color: #fff;\r\n}\r\n\r\n.v-text-field__slot {\r\n  padding: 0px !important; /* Remove qualquer padding extra do Vuetify */\r\n}\r\n\r\n.divider-custom {\r\n  background-color: #fff;\r\n  margin-top: 10px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.v-divider {\r\n  border-right: 3px solid #fff !important; /* Controla a espessura do divisor com border */\r\n}\r\n\r\n.login-btn {\r\n  background-color: #00420c;\r\n  color: #fff;\r\n  border-radius: 7px;\r\n  width: 120px;\r\n  height: 45px;\r\n}\r\n\r\n/* Estilo para o formulário de login */\r\n.v-dialog .v-card {\r\n  background-color: #00420c;\r\n  color: #fff;\r\n}\r\n\r\n.text-center {\r\n  text-align: center;\r\n}\r\n\r\n.close-btn {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n}\r\n\r\n.confirm-btn {\r\n  width: 100%;\r\n  height: 45px;\r\n  border-radius: 5px;\r\n  font-weight: bold;\r\n  background-color: #007700;\r\n}\r\n\r\n.confirm-btn:hover {\r\n  background-color: #00420c;\r\n}\r\n</style>\r\n"],"mappings":"AA0EA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,