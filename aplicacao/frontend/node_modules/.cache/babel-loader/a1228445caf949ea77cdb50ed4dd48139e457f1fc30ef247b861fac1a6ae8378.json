{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useObraStore } from '../../store/obraStore';\nimport { onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  data() {\n    return {\n      tcc: {}\n    };\n  },\n  setup() {\n    const obraStore = useObraStore();\n    const route = useRoute();\n    const router = useRouter();\n\n    // Carregar os dados da obra com base no id_obra da URL\n    onMounted(async () => {\n      await obraStore.listarObras();\n      const obra = obraStore.obras.find(item => item.id_obra === parseInt(route.params.id_obra));\n      if (obra) {\n        this.tcc = {\n          title: obra.titulo,\n          summary: obra.resumo,\n          type: obra.tipo,\n          classification: obra.classificacao_tematica,\n          year: obra.ano,\n          date: obra.data_defesa,\n          author: obra.autores.join(', '),\n          // Assumindo que o autor é uma lista de nomes\n          advisor: obra.orientador,\n          keywords: obra.palavras_chave\n        };\n      }\n    });\n\n    // Função para voltar à tela de busca\n    const goBack = () => {\n      router.push('/SearchPage');\n    };\n    return {\n      tcc,\n      goBack\n    };\n  }\n};","map":{"version":3,"names":["useObraStore","onMounted","useRoute","useRouter","data","tcc","setup","obraStore","route","router","listarObras","obra","obras","find","item","id_obra","parseInt","params","title","titulo","summary","resumo","type","tipo","classification","classificacao_tematica","year","ano","date","data_defesa","author","autores","join","advisor","orientador","keywords","palavras_chave","goBack","push"],"sources":["C:\\Users\\Micro\\Music\\sistema_tcc\\aplicacao\\frontend\\src\\components\\lista\\TccSelecionado.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <!-- Título do Trabalho -->\r\n    <v-row class=\"section-title\" justify=\"center\">\r\n      <v-col cols=\"12\" md=\"7\">\r\n        <h3 class=\"tcc-title text-center\">{{ tcc.title }}</h3>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Cartão Maior -->\r\n    <v-row justify=\"center\" class=\"mt-3\">\r\n      <v-col cols=\"12\" md=\"7\">\r\n        <v-card class=\"tcc-card-main\">\r\n          <v-card-text>\r\n            <!-- Resumo -->\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"7\">\r\n                <p class=\"tcc-summary\">\r\n                  <strong>RESUMO:</strong> {{ tcc.summary }}\r\n                </p>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <!-- Botão Download -->\r\n            <v-row>\r\n              <v-col cols=\"12\" class=\"text-center\">\r\n                <v-btn class=\"download-btn\">Baixar Obra Completa</v-btn>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <!-- Cards Individuais -->\r\n            <v-row>\r\n              <v-col cols=\"12\" class=\"mt-3\">\r\n                <v-card class=\"tcc-card\">\r\n                  <v-card-text>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Tipo de Obra:</strong> <br /> {{ tcc.type }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Classificação Temática:</strong> <br /> {{ tcc.classification }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Ano:</strong> <br /> {{ tcc.year }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Data de Defesa:</strong> <br /> {{ tcc.date }}\r\n                    </p>\r\n                  </v-card-text>\r\n                </v-card>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" class=\"mt-3\">\r\n                <v-card class=\"tcc-card\">\r\n                  <v-card-text>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Autor(es):</strong> <br /> {{ tcc.author }}\r\n   