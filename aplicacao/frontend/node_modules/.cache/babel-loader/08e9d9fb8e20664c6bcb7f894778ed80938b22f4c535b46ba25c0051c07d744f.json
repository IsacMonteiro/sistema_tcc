{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useObraStore } from '../../store/obraStore';\nimport { onMounted, ref } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const obraStore = useObraStore();\n    const route = useRoute();\n    const router = useRouter();\n\n    // Usando ref para tornar a variável reativa\n    const tcc = ref({\n      title: '',\n      summary: '',\n      type: '',\n      classification: '',\n      year: '',\n      date: '',\n      author: '',\n      advisor: '',\n      keywords: []\n    });\n\n    // Carregar os dados da obra com base no id_obra da URL\n    onMounted(async () => {\n      await obraStore.listarObras();\n      const obra = obraStore.obras.find(item => item.id_obra === parseInt(route.params.id_obra));\n      if (obra) {\n        tcc.value = {\n          title: obra.titulo,\n          summary: obra.resumo,\n          type: obra.tipo,\n          classification: obra.classificacao_tematica,\n          year: obra.ano,\n          date: obra.data_defesa,\n          author: obra.autores.join(', '),\n          // Assumindo que o autor é uma lista de nomes\n          advisor: obra.orientador,\n          keywords: obra.palavras_chave\n        };\n      }\n    });\n\n    // Função para voltar à tela de busca\n    const goBack = () => {\n      router.push('/SearchPage');\n    };\n    return {\n      tcc,\n      goBack\n    };\n  }\n};","map":{"version":3,"names":["useObraStore","onMounted","ref","useRoute","useRouter","setup","obraStore","route","router","tcc","title","summary","type","classification","year","date","author","advisor","keywords","listarObras","obra","obras","find","item","id_obra","parseInt","params","value","titulo","resumo","tipo","classificacao_tematica","ano","data_defesa","autores","join","orientador","palavras_chave","goBack","push"],"sources":["C:\\Users\\Micro\\Music\\sistema_tcc\\aplicacao\\frontend\\src\\components\\lista\\TccSelecionado.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <!-- Título do Trabalho -->\r\n    <v-row class=\"section-title\" justify=\"center\">\r\n      <v-col cols=\"12\" md=\"7\">\r\n        <h3 class=\"tcc-title text-center\">{{ tcc.title }}</h3>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Cartão Maior -->\r\n    <v-row justify=\"center\" class=\"mt-3\">\r\n      <v-col cols=\"12\" md=\"7\">\r\n        <v-card class=\"tcc-card-main\">\r\n          <v-card-text>\r\n            <!-- Resumo -->\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"7\">\r\n                <p class=\"tcc-summary\">\r\n                  <strong>RESUMO:</strong> {{ tcc.summary }}\r\n                </p>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <!-- Botão Download -->\r\n            <v-row>\r\n              <v-col cols=\"12\" class=\"text-center\">\r\n                <v-btn class=\"download-btn\">Baixar Obra Completa</v-btn>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <!-- Cards Individuais -->\r\n            <v-row>\r\n              <v-col cols=\"12\" class=\"mt-3\">\r\n                <v-card class=\"tcc-card\">\r\n                  <v-card-text>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Tipo de Obra:</strong> <br /> {{ tcc.type }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Classificação Temática:</strong> <br /> {{ tcc.classification }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Ano:</strong> <br /> {{ tcc.year }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Data de Defesa:</strong> <br /> {{ tcc.date }}\r\n                    </p>\r\n                  </v-card-text>\r\n                </v-card>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" class=\"mt-3\">\r\n                <v-card class=\"tcc-card\">\r\n                  <v-card-text>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Autor(es):</strong> <br /> {{ tcc.author }}\r\n                    </p>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Orientador:</strong> <br /> {{ tcc.advisor }}\r\n                    </p>\r\n                  </v-card-text>\r\n                </v-card>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" class=\"mt-3\">\r\n                <v-card class=\"tcc-card\">\r\n                  <v-card-text>\r\n                    <p class=\"tcc-section\">\r\n                      <strong>Palavras-Chave:</strong> <br /> {{ tcc.keywords.join(\", \") }}\r\n                    </p>\r\n                  </v-card-text>\r\n                </v-card>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <!-- Botão Voltar -->\r\n            <v-row justify=\"center\" class=\"mt-4\">\r\n              <v-col cols=\"12\" class=\"text-center mt-2\">\r\n                <v-btn class=\"back-btn\" @click=\"goBack\">Voltar</v-btn>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { useObraStore } from '../../store/obraStore';\r\nimport { onMounted, ref } from 'vue';\r\nimport { useRoute, useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  setup() {\r\n    const obraStore = useObraStore();\r\n    const route = useRoute();\r\n    const router = useRouter();\r\n\r\n    // Usando ref para tornar a variável reativa\r\n    const tcc = ref({\r\n      title: '',\r\n      summary: '',\r\n      type: '',\r\n      classification: '',\r\n      year: '',\r\n      date: '',\r\n      author: '',\r\n      advisor: '',\r\n      keywords: [],\r\n    });\r\n\r\n    // Carregar os dados da obra com base no id_obra da URL\r\n    onMounted(async () => {\r\n      await obraStore.listarObras();\r\n      const obra = obraStore.obras.find(\r\n        (item) => item.id_obra === parseInt(route.params.id_obra)\r\n      );\r\n      if (obra) {\r\n        tcc.value = {\r\n          title: obra.titulo,\r\n          summary: obra.resumo,\r\n          type: obra.tipo,\r\n          classification: obra.classificacao_tematica,\r\n          year: obra.ano,\r\n          date: obra.data_defesa,\r\n          author: obra.autores.join(', '), // Assumindo que o autor é uma lista de nomes\r\n          advisor: obra.orientador,\r\n          keywords: obra.palavras_chave,\r\n        };\r\n      }\r\n    });\r\n\r\n    // Função para voltar à tela de busca\r\n    const goBack = () => {\r\n      router.push('/SearchPage');\r\n    };\r\n\r\n    return {\r\n      tcc,\r\n      goBack,\r\n    };\r\n  },\r\n};\r\n</script>\r\n  \r\n<style scoped>\r\n  /* Título */\r\n  .section-title h3 {\r\n    margin-top: 50px;\r\n    background: linear-gradient(90deg, #00420c, #28a745);\r\n    color: white;\r\n    padding: 15px;\r\n    border-radius: 5px;\r\n    text-transform: uppercase;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  /* Resumo */\r\n  .tcc-summary {\r\n    font-size: 16px;\r\n    margin-bottom: 20px;\r\n    line-height: 1.6;\r\n    text-align: justify;\r\n  }\r\n  \r\n  /* Cartão Maior */\r\n  .tcc-card-main {\r\n    background-color: #f4f4f4;\r\n    border-radius: 10px;\r\n    padding: 20px;\r\n  }\r\n  \r\n  /* Cartões Individuais */\r\n  .tcc-card {\r\n    background-color: #ffffff;\r\n    border-radius: 8px;\r\n    padding: 15px;\r\n  }\r\n  \r\n  /* Texto nos cartões */\r\n  .tcc-section {\r\n    margin-bottom: 10px;\r\n    font-size: 16px;\r\n  }\r\n  \r\n  .tcc-section strong {\r\n    color: #004b81;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  /* Botões */\r\n  .download-btn {\r\n    background-color: #00420c;\r\n    border-radius: 7px;\r\n    color: white;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .download-btn:h