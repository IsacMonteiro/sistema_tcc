{"ast":null,"code":"import { useAutorStore } from \"../../store/autorStore\";\nimport { useOrientadorStore } from \"../../store/orientadorStore\";\nimport { useObraStore } from \"../../store/obraStore\";\nimport { useCursoStore } from \"../../store/cursoStore\";\nimport { onMounted, ref } from \"vue\";\nexport default {\n  setup() {\n    const autorStore = useAutorStore();\n    const orientadorStore = useOrientadorStore();\n    const obraStore = useObraStore();\n    const cursoStore = useCursoStore();\n    const autores = ref([]);\n    const orientadores = ref([]);\n    const cursos = ref([]);\n    const obras = ref([]);\n    const dropdownOptions = ref([[],\n    // Autor\n    [],\n    // Título\n    [],\n    // Curso\n    [],\n    // Ano\n    [],\n    // Orientador\n    [] // Tipo de Obra\n    ]);\n    const buttons = [\"Autor\", \"Título\", \"Áreas de Conhecimento - Cursos\", \"Data de Defesa\", \"Orientador\", \"Tipo de Obra\"];\n    const dropdowns = ref([false, false, false, false, false, false]);\n    const pages = ref([0, 0, 0, 0, 0, 0]); // Página atual para cada dropdown\n\n    const itemsPerPage = 7; // Número de itens por página\n\n    const toggleDropdown = index => {\n      dropdowns.value[index] = !dropdowns.value[index];\n    };\n\n    // Função para formatar o nome conforme o desejado\n    const formatName = fullName => {\n      const nameParts = fullName.split(\" \");\n      const lastName = nameParts.pop();\n      const firstName = nameParts.join(\" \");\n      return `${lastName}, ${firstName}`;\n    };\n\n    // Função para agrupar e contar os itens\n    const groupAndCountByCustomRelation = (mapFn, items) => {\n      console.log(\"Agrupando itens:\", items); // Log do array de itens\n      const grouped = items.reduce((acc, item) => {\n        const key = mapFn(item);\n        console.log(\"Chave agrupada:\", {\n          item,\n          key\n        }); // Log da chave agrupada\n        if (!key) return acc;\n        acc[key] = (acc[key] || 0) + 1;\n        return acc;\n      }, {});\n      return Object.entries(grouped).map(([text, count]) => ({\n        text,\n        count\n      }));\n    };\n\n    // Atualiza os dropdowns após carregar os dados\n    const updateDropdowns = () => {\n      console.log(\"Atualizando dropdowns...\");\n      dropdownOptions.value[0] = groupAndCountByCustomRelation(obra => {\n        const autor = autores.value.find(a => a.id_autor === obra.fk_id_autor);\n        console.log(\"Associando autor à obra:\", {\n          obra,\n          autor\n        }); // Log para depuração\n        return autor ? autor.nome_autor : \"Autor Desconhecido\";\n      }, obras.value);\n      console.log(\"Dropdown de autores:\", dropdownOptions.value[0]); // Log do dropdown de autores\n\n      dropdownOptions.value[4] = groupAndCountByCustomRelation(obra => {\n        const orientador = orientadores.value.find(o => o.id_orientador === obra.fk_id_orientador);\n        console.log(\"Associando orientador à obra:\", {\n          obra,\n          orientador\n        }); // Log para depuração\n        return orientador ? orientador.nome_orientador : \"Orientador Desconhecido\";\n      }, obras.value);\n      console.log(\"Dropdown de orientadores:\", dropdownOptions.value[4]); // Log do dropdown de orientadores\n    };\n\n    // Função para obter os itens visíveis da página atual\n    const getVisibleItems = index => {\n      const start = pages.value[index] * itemsPerPage;\n      const visibleItems = dropdownOptions.value[index].slice(start, start + itemsPerPage);\n      console.log(\"Itens visíveis:\", {\n        index,\n        visibleItems\n      }); // Log dos itens visíveis\n      return visibleItems;\n    };\n\n    // Verifica se há uma página anterior\n    const hasPreviousPage = index => {\n      const hasPrevious = pages.value[index] > 0;\n      console.log(\"Tem página anterior?\", {\n        index,\n        hasPrevious\n      }); // Log da verificação\n      return hasPrevious;\n    };\n\n    // Verifica se há uma página seguinte\n    const hasNextPage = index => {\n      const totalItems = dropdownOptions.value[index].length;\n      const hasNext = pages.value[index] * itemsPerPage + itemsPerPage < totalItems;\n      console.log(\"Tem próxima página?\", {\n        index,\n        hasNext\n      }); // Log da verificação\n      return hasNext;\n    };\n\n    // Vai para a página anterior\n    const goToPreviousPage = index => {\n      if (pages.value[index] > 0) {\n        pages.value[index]--;\n        console.log(\"Indo para a página anterior:\", {\n          index,\n          page: pages.value[index]\n        }); // Log de navegação\n      }\n    };\n\n    // Vai para a próxima página\n    const goToNextPage = index => {\n      const totalItems = dropdownOptions.value[index].length;\n      if (pages.value[index] * itemsPerPage + itemsPerPage < totalItems) {\n        pages.value[index]++;\n        console.log(\"Indo para a próxima página:\", {\n          index,\n          page: pages.value[index]\n        }); // Log de navegação\n      }\n    };\n    onMounted(async () => {\n      try {\n        console.log(\"Montando componente e carregando dados...\");\n        await autorStore.listarAutores();\n        autores.value = autorStore.autores;\n        console.log(\"Autores carregados:\", autores.value); // Log de autores carregados\n\n        await orientadorStore.listarOrientadores();\n        orientadores.value = orientadorStore.orientadores;\n        console.log(\"Orientadores carregados:\", orientadores.value); // Log de orientadores carregados\n\n        await obraStore.listarObras();\n        obras.value = obraStore.obras;\n        console.log(\"Obras carregadas:\", obras.value); // Log de obras carregadas\n\n        await cursoStore.listarCursos();\n        cursos.value = cursoStore.cursos;\n        console.log(\"Cursos carregados:\", cursos.value); // Log de cursos carregados\n\n        updateDropdowns();\n      } catch (error) {\n        console.error(\"Erro ao carregar dados:\", error); // Log de erro\n      }\n    });\n    return {\n      autores,\n      orientadores,\n      cursos,\n      obras,\n      dropdownOptions,\n      buttons,\n      dropdowns,\n      toggleDropdown,\n      formatName,\n      getVisibleItems,\n      hasPreviousPage,\n      hasNextPage,\n      goToPreviousPage,\n      goToNextPage\n    };\n  }\n};","map":{"version":3,"names":["useAutorStore","useOrientadorStore","useObraStore","useCursoStore","onMounted","ref","setup","autorStore","orientadorStore","obraStore","cursoStore","autores","orientadores","cursos","obras","dropdownOptions","buttons","dropdowns","pages","itemsPerPage","toggleDropdown","index","value","formatName","fullName","nameParts","split","lastName","pop","firstName","join","groupAndCountByCustomRelation","mapFn","items","console","log","grouped","reduce","acc","item","key","Object","entries","map","text","count","updateDropdowns","obra","autor","find","a","id_autor","fk_id_autor","nome_autor","orientador","o","id_orientador","fk_id_orientador","nome_orientador","getVisibleItems","start","visibleItems","slice","hasPreviousPage","hasPrevious","hasNextPage","totalItems","length","hasNext","goToPreviousPage","page","goToNextPage","listarAutores","listarOrientadores","listarObras","listarCursos","error"],"sources":["C:\\Users\\Micro\\Music\\sistema_tcc\\aplicacao\\frontend\\src\\components\\lista\\ListaSuspensa.vue"],"sourcesContent":["<template>\r\n  <v-container class=\"d-flex flex-column align-center gap-4\">\r\n    <!-- Botões com Lista Suspensa -->\r\n    <v-row class=\"button-container\" justify=\"center\" align=\"center\">\r\n      <v-col\r\n        v-for=\"(item, index) in buttons\"\r\n        :key=\"index\"\r\n        cols=\"12\"\r\n        md=\"4\"\r\n        class=\"mb-4\"\r\n      >\r\n        <!-- Botão Principal -->\r\n        <v-btn\r\n          class=\"filter-button\"\r\n          elevation=\"2\"\r\n          dark\r\n          @click=\"toggleDropdown(index)\"\r\n        >\r\n          <!-- Ícone à Esquerda -->\r\n          <v-icon class=\"me-2\">\r\n            {{ dropdowns[index] ? 'mdi-minus' : 'mdi-plus' }}\r\n          </v-icon>\r\n          <span>{{ item }}</span>\r\n        </v-btn>\r\n\r\n        <!-- Lista Suspensa -->\r\n        <v-expand-transition>\r\n          <div v-if=\"dropdowns[index]\" class=\"dropdown-list\">\r\n            <ul v-if=\"index === 0\">\r\n              <!-- Testando diretamente a renderização dos autores -->\r\n              <li v-for=\"(autor, idx) in dropdownOptions[index]\" :key=\"idx\">\r\n                {{ autor.text }} \r\n                <span class=\"item-count\">{{ autor.count }}</span>\r\n              </li>\r\n            </ul>\r\n\r\n            <ul v-if=\"index === 1\">\r\n              <li v-for=\"(obra, idx) in getVisibleItems(index)\" :key=\"idx\">\r\n                {{ obra.text }} \r\n                <span class=\"item-count\">{{ obra.count }}</span>\r\n              </li>\r\n            </ul>\r\n\r\n            <ul v-if=\"index === 2\">\r\n              <li v-for=\"(curso, idx) in getVisibleItems(index)\" :key=\"idx\">\r\n                {{ curso.text }} \r\n                <span class=\"item-count\">{{ curso.count }}</span>\r\n              </li>\r\n            </ul>\r\n\r\n            <ul v-if=\"index === 3\">\r\n              <li v-for=\"(ano, idx) in getVisibleItems(index)\" :key=\"idx\">\r\n                {{ ano.text }} \r\n                <span class=\"item-count\">{{ ano.count }}</span>\r\n              </li>\r\n            </ul>\r\n\r\n            <ul v-if=\"index === 4\">\r\n              <li v-for=\"(orientador, idx) in getVisibleItems(index)\" :key=\"idx\">\r\n                {{ formatName(orientador.text) }} \r\n                <span class=\"item-count\">{{ orientador.count }}</span>\r\n              </li>\r\n            </ul>\r\n\r\n            <ul v-if=\"index === 5\">\r\n              <li v-for=\"(tipo, idx) in getVisibleItems(index)\" :key=\"idx\">\r\n                {{ tipo.text }} \r\n                <span class=\"item-count\">{{ tipo.count }}</span>\r\n              </li>\r\n            </ul>\r\n\r\n            <!-- Botões para navegar entre as páginas -->\r\n            <div v-if=\"dropdowns[index]\" class=\"pagination-buttons\">\r\n              <v-btn\r\n                v-if=\"hasPreviousPage(index)\"\r\n                @click=\"goToPreviousPage(index)\"\r\n                class=\"pagination-button prev-button\"\r\n              >\r\n                Voltar\r\n              </v-btn>\r\n\r\n              <v-btn\r\n                v-if=\"hasNextPage(index)\"\r\n                @click=\"goToNextPage(index)\"\r\n                class=\"pagination-button next-button\"\r\n              >\r\n                Próximo\r\n              </v-btn>\r\n            </div>\r\n          </div>\r\n        </v-expand-transition>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { useAutorStore } from \"../../store/autorStore\";\r\nimport { useOrientadorStore } from \"../../store/orientadorStore\"; \r\nimport { useObraStore } from \"../../store/obraStore\"; \r\nimport { useCursoStore } from \"../../store/cursoStore\"; \r\nimport { onMounted, ref } from \"vue\";\r\n\r\nexport default {\r\n  setup() {\r\n    const autorStore = useAutorStore();\r\n    const orientadorStore = useOrientadorStore();\r\n    const obraStore = useObraStore();\r\n    const cursoStore = useCursoStore();\r\n    const autores = ref([]);\r\n    const orientadores = ref([]);\r\n    const cursos = ref([]);\r\n    const obras = ref([]);\r\n\r\n    const dropdownOptions = ref([\r\n      [], // Autor\r\n      [], // Título\r\n      [], // Curso\r\n      [], // Ano\r\n      [], // Orientador\r\n      [], // Tipo de Obra\r\n    ]);\r\n\r\n    const buttons = [\r\n      \"Autor\",\r\n      \"Título\",\r\n      \"Áreas de Conhecimento - Cursos\",\r\n      \"Data de Defesa\",\r\n      \"Orientador\",\r\n      \"Tipo de Obra\",\r\n    ];\r\n\r\n    const dropdowns = ref([false, false, false, false, false, false]);\r\n    const pages = ref([0, 0, 0, 0, 0, 0]); // Página atual para cada dropdown\r\n\r\n    const itemsPerPage = 7; // Número de itens por página\r\n\r\n    const toggleDropdown = (index) => {\r\n      dropdowns.value[index] = !dropdowns.value[index];\r\n    };\r\n\r\n    // Função para formatar o nome conforme o desejado\r\n    const formatName = (fullName) => {\r\n      const nameParts = fullName.split(\" \");\r\n      const lastName = nameParts.pop();\r\n      const firstName = nameParts.join(\" \");\r\n      return `${lastName}, ${firstName}`;\r\n    };\r\n\r\n    // Função para agrupar e contar os itens\r\n    const groupAndCountByCustomRelation = (mapFn, items) => {\r\n      console.log(\"Agrupando itens:\", items); // Log do array de itens\r\n      const grouped = items.reduce((acc, item) => {\r\n        const key = mapFn(item);\r\n        console.log(\"Chave agrupada:\", { item, key }); // Log da chave agrupada\r\n        if (!key) return acc;\r\n        acc[key] = (acc[key] || 0) + 1;\r\n        return acc;\r\n      }, {});\r\n\r\n      return Object.entries(grouped).map(([text, count]) => ({\r\n        text, count\r\n      }));\r\n    };\r\n\r\n    // Atualiza os dropdowns após carregar os dados\r\n    const updateDropdowns = () => {\r\n      console.log(\"Atualizando dropdowns...\");\r\n\r\n      dropdownOptions.value[0] = groupAndCountByCustomRelation(\r\n        (obra) => {\r\n          const autor = autores.value.find(a => a.id_autor === obra.fk_id_autor);\r\n          console.log(\"Associando autor à obra:\", { obra, autor }); // Log para depuração\r\n          return autor ? autor.nome_autor : \"Autor Desconhecido\";\r\n        },\r\n        obras.value\r\n      );\r\n      console.log(\"Dropdown de autores:\", dropdownOptions.value[0]); // Log do dropdown de autores\r\n\r\n      dropdownOptions.value[4] = groupAndCountByCustomRelation(\r\n        (obra) => {\r\n          const orientador = orientadores.value.find(o => o.id_orientador === obra.fk_id_orientador);\r\n          console.log(\"Associando orientador à obra:\", { obra, orientador }); // Log para depuração\r\n          return orientador ? orientador.nome_orientador : \"Orientador Desconhecido\";\r\n        },\r\n        obras.value\r\n      );\r\n      console.log(\"Dropdown de orientadores:\", dropdownOptions.value[4]); // Log do dropdown de orientadores\r\n    };\r\n\r\n    // Função para obter os itens visíveis da página atual\r\n    const getVisibleItems = (index) => {\r\n      const start = pages.value[index] * itemsPerPage;\r\n      const visibleItems = dropdownOptions.value[index].slice(start, start + itemsPerPage);\r\n      console.log(\"Itens visíveis:\", { index, visibleItems }); // Log dos itens visíveis\r\n      return visibleItems;\r\n    };\r\n\r\n    // Verifica se há uma página anterior\r\n    const hasPreviousPage = (index) => {\r\n      const hasPrevious = pages.value[index] > 0;\r\n      console.log(\"Tem página anterior?\", { index, hasPrevious }); // Log da verificação\r\n      return hasPrevious;\r\n    };\r\n\r\n    // Verifica se há uma página seguinte\r\n    const hasNextPage = (index) => {\r\n      const totalItems = dropdownOptions.value[index].length;\r\n      const hasNext = pages.value[index] * itemsPerPage + itemsPerPage < totalItems;\r\n      console.log(\"Tem próxima página?\", { index, hasNext }); // Log da verificação\r\n      return hasNext;\r\n    };\r\n\r\n    // Vai para a página anterior\r\n    const goToPreviousPage = (index) => {\r\n      if (pages.value[index] > 0) {\r\n        pages.value[index]--;\r\n        console.log(\"Indo para a página anterior:\", { index, page: pages.value[index] }); // Log de navegação\r\n      }\r\n    };\r\n\r\n    // Vai para a próxima página\r\n    const goToNextPage = (index) => {\r\n      const totalItems = dropdownOptions.value[index].length;\r\n      if (pages.value[index] * i