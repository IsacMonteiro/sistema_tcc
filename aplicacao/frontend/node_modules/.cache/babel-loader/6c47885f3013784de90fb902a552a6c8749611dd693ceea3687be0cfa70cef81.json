{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed, onMounted } from \"vue\";\nimport { useAutorStore } from \"../../store/autorStore\";\nimport { useObraStore } from \"../../store/obraStore\";\nimport { useOrientadorStore } from \"../../store/orientadorStore\"; // Adicionar store de orientadores\nimport { useRouter } from \"vue-router\";\nexport default {\n  setup() {\n    const router = useRouter();\n    const autorStore = useAutorStore();\n    const obraStore = useObraStore();\n    const orientadorStore = useOrientadorStore(); // Store de orientadores\n\n    const results = ref([]);\n    const currentPage = ref(1);\n    const itemsPerPage = 10;\n    const filterKey = ref(\"\"); // Tipo de filtro (ex.: \"Orientador\", \"Curso\")\n    const selectedItem = ref(\"\"); // Item selecionado no dropdown\n\n    const filteredResults = computed(() => {\n      const allResults = results.value.map(obra => {\n        const autor = autorStore.autores.find(autor => autor.id_autor === obra.fk_id_autor);\n\n        // Adicionar o nome do orientador às obras\n        const orientador = orientadorStore.orientadores.find(orientador => orientador.id_orientador === obra.fk_id_orientador);\n        return {\n          ...obra,\n          author: autor ? autor.nome_autor : \"Autor desconhecido\",\n          orientador: orientador ? orientador.nome_orientador : \"Orientador desconhecido\"\n        };\n      });\n      console.log(\"Dados antes do filtro:\", allResults);\n      if (!filterKey.value || !selectedItem.value) {\n        console.log(\"Sem filtro aplicado\");\n        return allResults;\n      }\n      const filtered = allResults.filter(obra => {\n        switch (filterKey.value) {\n          case \"Orientador\":\n            const obraOrientador = obra.orientador?.toLowerCase().trim();\n            const selectedOrientador = selectedItem.value.toLowerCase().trim();\n            console.log(\"Comparando orientador:\", {\n              obraOrientador,\n              selectedOrientador\n            });\n            return obraOrientador === selectedOrientador;\n          case \"Curso\":\n            const obraCurso = obra.curso?.toLowerCase().trim();\n            const selectedCurso = selectedItem.value.toLowerCase().trim();\n            console.log(\"Comparando curso:\", {\n              obraCurso,\n              selectedCurso\n            });\n            return obraCurso === selectedCurso;\n          default:\n            return true;\n        }\n      });\n      console.log(\"Filtro aplicado:\", {\n        filterKey: filterKey.value,\n        selectedItem: selectedItem.value\n      });\n      console.log(\"Resultados filtrados:\", filtered);\n      return filtered;\n    });\n    const paginatedResults = computed(() => {\n      const start = (currentPage.value - 1) * itemsPerPage;\n      const end = start + itemsPerPage;\n      return filteredResults.value.slice(start, end);\n    });\n    const totalPages = computed(() => {\n      return Math.ceil(filteredResults.value.length / itemsPerPage);\n    });\n    const nextPage = () => {\n      if (currentPage.value < totalPages.value) {\n        currentPage.value++;\n      }\n    };\n    const previousPage = () => {\n      if (currentPage.value > 1) {\n        currentPage.value--;\n      }\n    };\n    const loadData = async () => {\n      await Promise.all([obraStore.listarObras(), autorStore.listarAutores(), orientadorStore.listarOrientadores() // Carregar orientadores\n      ]);\n      results.value = obraStore.obras;\n      console.log(\"Obras carregadas:\", results.value);\n    };\n    const formatDate = dateString => {\n      if (!dateString) return \"\";\n      const date = new Date(dateString);\n      if (isNaN(date)) return \"\";\n      const options = {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return new Intl.DateTimeFormat(\"pt-BR\", options).format(date);\n    };\n    const goBack = () => {\n      router.push(\"/\");\n    };\n    onMounted(loadData);\n    return {\n      results,\n      filteredResults,\n      paginatedResults,\n      currentPage,\n      totalPages,\n      nextPage,\n      previousPage,\n      formatDate,\n      goBack,\n      filterKey,\n      selectedItem\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useAutorStore","useObraStore","useOrientadorStore","useRouter","setup","router","autorStore","obraStore","orientadorStore","results","currentPage","itemsPerPage","filterKey","selectedItem","filteredResults","allResults","value","map","obra","autor","autores","find","id_autor","fk_id_autor","orientador","orientadores","id_orientador","fk_id_orientador","author","nome_autor","nome_orientador","console","log","filtered","filter","obraOrientador","toLowerCase","trim","selectedOrientador","obraCurso","curso","selectedCurso","paginatedResults","start","end","slice","totalPages","Math","ceil","length","nextPage","previousPage","loadData","Promise","all","listarObras","listarAutores","listarOrientadores","obras","formatDate","dateString","date","Date","isNaN","options","year","month","day","Intl","DateTimeFormat","format","goBack","push"],"sources":["C:\\Users\\Micro\\Music\\sistema_tcc\\aplicacao\\frontend\\src\\components\\lista\\ListaResultados.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <v-row class=\"section-title\" justify=\"center\">\r\n      <v-col cols=\"12\" md=\"7\">\r\n        <h4 class=\"text-center\">RESULTADO DA BUSCA</h4>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <v-row justify=\"center\">\r\n      <v-col cols=\"12\" md=\"7\">\r\n        <v-card\r\n          v-for=\"(result, index) in paginatedResults\"\r\n          :key=\"index\"\r\n          class=\"mb-3 result-card\"\r\n        >\r\n          <v-card-text>\r\n            <h5 class=\"result-title\">\r\n              <router-link\r\n                :to=\"{ name: 'TccPage', params: { id_obra: result.id_obra } }\"\r\n                class=\"result-title\"\r\n              >\r\n                {{ result.titulo }}\r\n              </router-link>\r\n            </h5>\r\n            <p class=\"result-author\">\r\n              <strong>AUTOR(ES):</strong> {{ result.author }}\r\n            </p>\r\n            <p class=\"result-date\">\r\n              <strong>Data de Defesa:</strong> {{ formatDate(result.data_apresentacao) }}\r\n            </p>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <v-row justify=\"center\">\r\n      <v-col cols=\"auto\">\r\n        <v-btn\r\n          :disabled=\"currentPage === 1\"\r\n          @click=\"previousPage\"\r\n          class=\"mb-3 mr-2\"\r\n        >\r\n          Voltar\r\n        </v-btn>\r\n        <v-btn\r\n          :disabled=\"currentPage === totalPages\"\r\n          @click=\"nextPage\"\r\n          class=\"mb-3\"\r\n        >\r\n          Próximo\r\n        </v-btn>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <v-row justify=\"center\">\r\n      <v-col cols=\"auto\">\r\n        <v-btn\r\n          @click=\"goBack\"\r\n          id=\"back-button\"\r\n          class=\"mb-3\"\r\n        >\r\n          Voltar para a tela principal\r\n        </v-btn>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from \"vue\";\r\nimport { useAutorStore } from \"../../store/autorStore\";\r\nimport { useObraStore } from \"../../store/obraStore\";\r\nimport { useOrientadorStore } from \"../../store/orientadorStore\"; // Adicionar store de orientadores\r\nimport { useRouter } from \"vue-router\";\r\n\r\nexport default {\r\n  setup() {\r\n    const router = useRouter();\r\n\r\n    const autorStore = useAutorStore();\r\n    const obraStore = useObraStore();\r\n    const orientadorStore = useOrientadorStore(); // Store de orientadores\r\n\r\n    const results = ref([]);\r\n    const currentPage = ref(1);\r\n    const itemsPerPage = 10;\r\n\r\n    const filterKey = ref(\"\"); // Tipo de filtro (ex.: \"Orientador\", \"Curso\")\r\n    const selectedItem = ref(\"\"); // Item selecionado no dropdown\r\n\r\n    const filteredResults = computed(() => {\r\n      const allResults = results.value.map((obra) => {\r\n        const autor = autorStore.autores.find(\r\n          (autor) => autor.id_autor === obra.fk_id_autor\r\n        );\r\n\r\n        // Adicionar o nome do orientador às obras\r\n        const orientador = orientadorStore.orientadores.find(\r\n          (orientador) => orientador.id_orientador === obra.fk_id_orientador\r\n        );\r\n\r\n        return {\r\n          ...obra,\r\n          author: autor ? autor.nome_autor : \"Autor desconhecido\",\r\n          orientador: orientador ? orientador.nome_orientador : \"Orientador desconhecido\",\r\n        };\r\n      });\r\n\r\n      console.log(\"Dados antes do filtro:\", allResults);\r\n\r\n      if (!filterKey.value || !selectedItem.value) {\r\n        console.log(\"Sem filtro aplicado\");\r\n        return allResults;\r\n      }\r\n\r\n      const filtered = allResults.filter((obra) => {\r\n        switch (filterKey.value) {\r\n          case \"Orientador\":\r\n            const obraOrientador = obra.orientador?.toLowerCase().trim();\r\n            const selectedOrientador = selectedItem.value.toLowerCase().trim();\r\n            console.log(\"Comparando orientador:\", { obraOrientador, selectedOrientador });\r\n            return obraOrientador === selectedOrientador;\r\n\r\n          case \"Curso\":\r\n            const obraCurso = obra.curso?.toLowerCase().trim();\r\n            const selectedCurso = selectedItem.value.toLowerCase().trim();\r\n            console.log(\"Comparando curso:\", { obraCurso, selectedCurso });\r\n            return obraCurso === selectedCurso;\r\n\r\n          default:\r\n            return true;\r\n        }\r\n      });\r\n\r\n      console.log(\"Filtro aplicado:\", { filterKey: filterKey.value, selectedItem: selectedItem.value });\r\n      console.log(\"Resultados filtrados:\", filtered);\r\n\r\n      return filtered;\r\n    });\r\n\r\n    const paginatedResults = computed(() => {\r\n      const start = (currentPage.value - 1) * itemsPerPage;\r\n      const end = start + itemsPerPage;\r\n      return filteredResults.value.slice(start, end);\r\n    });\r\n\r\n    const totalPages = computed(() => {\r\n      return Math.ceil(filteredResults.value.length / itemsPerPage);\r\n    });\r\n\r\n    const nextPage = () => {\r\n      if (currentPage.value < totalPages.value) {\r\n        currentPage.value++;\r\n      }\r\n    };\r\n\r\n    const previousPage = () => {\r\n      if (currentPage.value > 1) {\r\n        currentPage.value--;\r\n      }\r\n    };\r\n\r\n    const loadData = async () => {\r\n      await Promise.all([\r\n        obraStore.listarObras(),\r\n        autorStore.listarAutores(),\r\n        orientadorStore.listarOrientadores(), // Carregar orientadores\r\n      ]);\r\n      results.value = obraStore.obras;\r\n      console.log(\"Obras carregadas:\", results.value);\r\n    };\r\n\r\n    const formatDate = (dateString) => {\r\n      if (!dateString) return \"\";\r\n      const date = new Date(dateString);\r\n      if (isNaN(date)) return \"\";\r\n      const options = { year: \"numeric\", month: \"long\", day: \"numeric\" };\r\n      return new Intl.DateTimeFormat(\"pt-BR\", options).format(date);\r\n    };\r\n\r\n    const goBack = () => {\r\n      router.push(\"/\");\r\n    };\r\n\r\n    onMounted(loadData);\r\n\r\n    return {\r\n      results,\r\n      filteredResults,\r\n      paginatedResults,\r\n      currentPage,\r\n      totalPages,\r\n      nextPage,\r\n      previousPage,\r\n      formatDate,\r\n      goBack,\r\n      filterKey,\r\n      selectedItem,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.section-title h4 {\r\n  margin-top: 50px;\r\n  background: linear-gradient(90deg, #00420c, #28a745);\r\n  color: white;\r\n  padding: 15px;\r\n  border