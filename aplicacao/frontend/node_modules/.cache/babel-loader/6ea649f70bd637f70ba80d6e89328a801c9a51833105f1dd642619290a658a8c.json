{"ast":null,"code":"export default {\n  data() {\n    return {\n      matricula: '',\n      senha: '',\n      isValid: false,\n      searchQuery: '',\n      rules: {\n        required: value => !!value || 'Este campo é obrigatório'\n      },\n      tccs: [{\n        id: 1,\n        title: 'TCC 1',\n        author: 'Autor 1'\n      }, {\n        id: 2,\n        title: 'TCC 2',\n        author: 'Autor 2'\n      }, {\n        id: 3,\n        title: 'TCC 3',\n        author: 'Autor 3'\n      }, {\n        id: 4,\n        title: 'TCC 4',\n        author: 'Autor 4'\n      }, {\n        id: 5,\n        title: 'TCC 5',\n        author: 'Autor 5'\n      }]\n    };\n  },\n  computed: {\n    filteredTCCs() {\n      return this.tccs.filter(tcc => tcc.title.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    login() {\n      // Implementar lógica de login\n      if (this.matricula && this.senha) {\n        console.log('Login realizado com sucesso!');\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","matricula","senha","isValid","searchQuery","rules","required","value","tccs","id","title","author","computed","filteredTCCs","filter","tcc","toLowerCase","includes","methods","login","console","log"],"sources":["C:\\Users\\Micro\\Music\\sistema_tcc\\aplicacao\\frontend\\src\\views\\HomePage.vue"],"sourcesContent":["<template>\r\n  <v-app>\r\n    <!-- Cabeçalho -->\r\n    <v-app-bar de>\r\n      <v-container>\r\n        <v-row align=\"center\" justify=\"space-between\">\r\n          <!-- Logo -->\r\n          <v-col cols=\"auto\">\r\n            <v-img src=\"/img/logo2.png\" alt=\"Logo\" max-width=\"100\"></v-img>\r\n          </v-col>\r\n\r\n          <!-- Login -->\r\n          <v-col cols=\"auto\">\r\n            <v-form ref=\"loginForm\" v-model=\"isValid\" class=\"d-flex align-center\">\r\n              <v-text-field\r\n                v-model=\"matricula\"\r\n                label=\"Matrícula\"\r\n                placeholder=\"Matrícula\"\r\n                :rules=\"[rules.required]\"\r\n                outlined\r\n                dense\r\n                class=\"mr-2\"\r\n              ></v-text-field>\r\n              <v-text-field\r\n                v-model=\"senha\"\r\n                label=\"Senha\"\r\n                type=\"password\"\r\n                placeholder=\"Senha\"\r\n                :rules=\"[rules.required]\"\r\n                outlined\r\n                dense\r\n                class=\"mr-2\"\r\n              ></v-text-field>\r\n              <v-btn\r\n                :disabled=\"!isValid\"\r\n                @click=\"login\"\r\n                color=\"secondary\"\r\n                class=\"mt-3\"\r\n              >\r\n                Entrar\r\n              </v-btn>\r\n            </v-form>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-app-bar>\r\n\r\n    <!-- Pilar (Área Principal de Conteúdo) -->\r\n    <v-main>\r\n      <v-container class=\"d-flex justify-center align-center\" style=\"min-height: 80vh;\">\r\n        <v-row align=\"center\" justify=\"center\">\r\n          <v-col cols=\"12\" md=\"8\" lg=\"6\">\r\n            <!-- Título -->\r\n            <v-container class=\"my-5 text-center\">\r\n              <v-row>\r\n                <v-col>\r\n                  <v-typography variant=\"h3\">Repositório de TCCs</v-typography>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n\r\n            <!-- Área de Pesquisa -->\r\n            <v-container>\r\n              <v-row>\r\n                <v-col cols=\"12\">\r\n                  <v-text-field\r\n                    v-model=\"searchQuery\"\r\n                    label=\"Pesquisar TCC\"\r\n                    outlined\r\n                    dense\r\n                    prepend-icon=\"mdi-magnify\"\r\n                  ></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n\r\n            <!-- Lista de TCCs -->\r\n            <v-container>\r\n              <v-row>\r\n                <v-col v-for=\"tcc in filteredTCCs\" :key=\"tcc.id\" cols=\"12\" md=\"4\">\r\n                  <v-card :href=\"'/tcc/' + tcc.id\" class=\"mb-4\">\r\n                    <v-card-title>{{ tcc.title }}</v-card-title>\r\n                    <v-card-subtitle>{{ tcc.author }}</v-card-subtitle>\r\n                    <v-card-actions>\r\n                      <v-btn text>Ver mais</v-btn>\r\n                    </v-card-actions>\r\n                  </v-card>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-main>\r\n\r\n    <!-- Rodapé -->\r\n    <v-footer app>\r\n      <v-container class=\"text-center\">\r\n        <v-row>\r\n          <v-col>\r\n            <span>&copy; 2024 Repositório Acadêmico - Todos os direitos reservados</span>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-footer>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      matricula: '',\r\n      senha: '',\r\n      isValid: false,\r\n      searchQuery: '',\r\n      rules: {\r\n        required: value => !!value || 'Este campo é obrigatório',\r\n      },\r\n      tccs: [\r\n        { id: 1, title: 'TCC 1', author: 'Autor 1' },\r\n        { id: 2, title: 'TCC 2', author: 'Autor 2' },\r\n        { id: 3, title: 'TCC 3', author: 'Autor 3' },\r\n        { id: 4, title: 'TCC 4', author: 'Autor 4' },\r\n        { id: 5, title: 'TCC 5', author: 'Autor 5' },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    filteredTCCs() {\r\n      return this.tccs.filter(tcc =>\r\n        tcc.title.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    login() {\r\n      // Implementar lógica de login\r\n      if (this.matricula && this.senha) {\r\n        console.log('Login realizado com sucesso!');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.v-app-bar {\r\n  background-color: #034811;\r\n}\r\n\r\n.v-footer {\r\n  background-color: #034811;\r\n  color: white;\r\n}\r\n\r\n.v-btn {\r\n  background-color: #034811;\r\n  color: white;\r\n}\r\n\r\n.v-btn[disabled] {\r\n  background-color: #034811;\r\n}\r\n\r\n.v-card {\r\n  transition: transform 0.3s ease-in-out;\r\n}\r\n\r\n.v-card:hover {\r\n  transform: scale(1.05);\r\n}\r\n</style>\r\n"],"mappings":"AA6GA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,KAAK;MACdC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;QACLC,QAAQ,EAAEC,KAAI,IAAK,CAAC,CAACA,KAAI,IAAK;MAChC,CAAC;MACDC,IAAI,EAAE,CACJ;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,OAAO;QAAEC,MAAM,EAAE;MAAU,CAAC,EAC5C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,OAAO;QAAEC,MAAM,EAAE;MAAU,CAAC,EAC5C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,OAAO;QAAEC,MAAM,EAAE;MAAU,CAAC,EAC5C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,OAAO;QAAEC,MAAM,EAAE;MAAU,CAAC,EAC5C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,OAAO;QAAEC,MAAM,EAAE;MAAU,CAAC;IAEhD,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACL,IAAI,CAACM,MAAM,CAACC,GAAE,IACxBA,GAAG,CAACL,KAAK,CAACM,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACb,WAAW,CAACY,WAAW,CAAC,CAAC,CACjE,CAAC;IACH;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN;MACA,IAAI,IAAI,CAAClB,SAAQ,IAAK,IAAI,CAACC,KAAK,EAAE;QAChCkB,OAAO,CAACC,GAAG,CAAC,8BAA8B,CAAC;MAC7C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}