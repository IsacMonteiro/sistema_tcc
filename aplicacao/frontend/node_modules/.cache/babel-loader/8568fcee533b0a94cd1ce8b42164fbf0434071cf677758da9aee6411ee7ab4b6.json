<label for=\"email_coorientador\">Email do Coorientador:</label>\r\n          <input type=\"email\" id=\"email_coorientador\" v-model=\"coorientador.email_coorientador\" />\r\n        </div>\r\n  \r\n        <!-- Campos do Curso -->\r\n        <h2>Dados do Curso</h2>\r\n        <div>\r\n          <label for=\"nome_curso\">Nome do Curso:</label>\r\n          <input type=\"text\" id=\"nome_curso\" v-model=\"curso.nome_curso\" required />\r\n        </div>\r\n  \r\n        <!-- Botão de Envio -->\r\n        <button type=\"submit\">Cadastrar Obra</button>\r\n        \r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        obra: {\r\n          titulo: '',\r\n          tipo: '',\r\n          data_apresentacao: '',\r\n          resumo: '',\r\n          palavras_chave: '',\r\n          arquivo: null,\r\n        },\r\n        autor: {\r\n          matricula_autor: '',\r\n          nome_autor: '',\r\n          email_autor: '',\r\n        },\r\n        orientador: {\r\n          matricula_orientador: '',\r\n          nome_orientador: '',\r\n          titulo_academico: '',\r\n          area_atuacao: '',\r\n          email_orientador: '',\r\n        },\r\n        coorientador: {\r\n          matricula_coorientador: '',\r\n          nome_coorientador: '',\r\n          titulo_academico: '',\r\n          area_atuacao: '',\r\n          email_coorientador: '',\r\n        },\r\n        curso: {\r\n          nome_curso: '',\r\n        },\r\n      };\r\n    },\r\n    methods: {\r\n      onFileChange(e) {\r\n        this.obra.arquivo = e.target.files[0];\r\n      },\r\n      submitForm() {\r\n        // Lógica de envio para o backend\r\n        console.log(\"Formulário enviado!\", this.obra, this.autor, this.orientador, this.coorientador, this.curso);\r\n        // Pode usar axios ou fetch para enviar os dados ao servidor\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Estilos po